# ==============================================
# AgentDNS environment configuration file
# This file overrides the default values in config.py
# ==============================================

# Database Configuration
DATABASE_URL=postgresql://agentdns:your_actual_password@localhost:5432/agentdns
REDIS_URL=redis://localhost:6379

# Milvus vector database configuration
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION_NAME=agentdns_services
MILVUS_DIMENSION=2560

# JWT Security Configuration (⚠️ Must be modified in the production environment)
SECRET_KEY=your-super-secret-key-change-this-in-production-$(date +%s)
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# OpenAI API Configuration ()
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_EMBEDDING_MODEL=text-embedding-3-large
OPENAI_MAX_TOKENS=8000

# Encrypted configuration (⚠️ Please use a real key, see generation instructions)
ENCRYPTION_KEY=your-encryption-key

# Environment configuration
ENVIRONMENT=development
DEBUG=true